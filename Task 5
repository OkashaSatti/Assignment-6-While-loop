#include<iostream>
using namespace std;
int main()
{
    int seat=1;
    int i=1;
    while(i<=5){
        cout<<"Row: "<<i<<endl<<"\t";
        int j=1;
        while(j<=7){
            cout<<"Seat: "<<seat<<"  ";
            seat=seat+1;
            j++;
        }
        cout<<endl;
        i++;
    }
}   
